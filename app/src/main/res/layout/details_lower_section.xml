<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/white"
    android:orientation="vertical"
    >
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/Resume_Headline_id"
        >

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="19dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:text="Resume Headline"
            android:textColor="#2C2C2C"
            android:textSize="20dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView4"
            android:layout_alignStart="@+id/textView4"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:text="Seeking an internship and currently in Jalandhar"
            android:layout_alignLeft="@+id/textView4" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/Key_Skills_id">

        <TextView
            android:id="@+id/textView44"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="19dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:text="Key Skills"
            android:textColor="#2C2C2C"
            android:textSize="20dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView56"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView44"
            android:layout_alignStart="@+id/textView44"
            android:layout_alignLeft="@+id/textView44"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="10dp"
            android:text="Node.js, Javascript, Android" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView56"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="24dp"
            android:layout_marginBottom="34dp"
            android:text="Add"
            android:textColor="@color/Button"
            android:textSize="17sp"
            android:textStyle="bold" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/Projects_id">

        <TextView
            android:id="@+id/textVew44"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="19dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:text="Projects"
            android:textColor="#2C2C2C"
            android:textSize="20dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/extView56"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textVew44"
            android:layout_alignStart="@+id/textVew44"
            android:layout_alignLeft="@+id/textVew44"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="10dp"
            android:text="Mess Management, ERP System, Weather Forecast" />

        <TextView
            android:id="@+id/textView666"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/extView56"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="24dp"
            android:layout_marginBottom="34dp"
            android:text="Add"
            android:textColor="@color/Button"
            android:textSize="17sp"
            android:textStyle="bold" />

    </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/Address_id">

            <TextView
                android:id="@+id/textV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="19dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:text="Address"
                android:textColor="#2C2C2C"
                android:textSize="20dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/View"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textV"
                android:layout_alignStart="@+id/textV"
                android:layout_alignLeft="@+id/textV"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="10dp"
                android:text="House No. X, Y Colony, New Delhi, India-182XX9" />

            <TextView
                android:id="@+id/textV6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/View"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginEnd="24dp"
                android:layout_marginRight="24dp"
                android:layout_marginBottom="34dp"
                android:text="Add"
                android:textColor="@color/Button"
                android:textSize="17sp"
                android:textStyle="bold" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/Languages_ID">

            <TextView
                android:id="@+id/Lang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="19dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:text="Languages Known"
                android:textColor="#2C2C2C"
                android:textSize="20dp"
                android:textStyle="bold"

                android:layout_marginBottom="10dp"/>
            <TextView
                android:id="@+id/lang_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Lang"
                android:layout_alignStart="@+id/Lang"
                android:layout_alignLeft="@+id/Lang"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="10dp"
                android:text="Turkey, Hindi" />

            <TextView
                android:id="@+id/add_language"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="24dp"
                android:layout_marginRight="24dp"
                android:text="Add"
                android:textColor="@color/Button"
                android:textSize="17sp"
                android:textStyle="bold" />

        </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/Resume_id">

        <TextView
            android:id="@+id/ResumeID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="19dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:text="Resume"
            android:textColor="#2C2C2C"
            android:textSize="20dp"
            android:textStyle="bold"

            android:layout_marginBottom="10dp"/>

        <TextView
            android:id="@+id/add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="24dp"
            android:text="Add Resume"
            android:textColor="@color/Button"
            android:textSize="17sp"
            android:textStyle="bold" />

    </RelativeLayout>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textColor="@android:color/white"
        android:text="Submit"
        android:background="@color/Button"
        android:padding="5dp"
        android:layout_margin="5dp"
        />

    </LinearLayout>
</ScrollView>